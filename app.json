{
  "expo": {
    "name": "Ayska-field-app",
    "slug": "Ayska-field-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ayskafieldapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Ayskafieldapp",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs location access to verify employee check-ins with doctors.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs location access to verify employee check-ins with doctors."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.Ayskafieldapp",
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Ayska.png",
          "imageWidth": 280,
          "resizeMode": "contain",
          "backgroundColor": "#F9FAFB",
          "dark": {
            "image": "./assets/images/Ayska.png",
            "backgroundColor": "#0F172A"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Ayska Field App to use your location to verify check-ins with doctors."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
